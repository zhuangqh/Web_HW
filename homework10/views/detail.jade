extends layout

block content
  #details
    #field
      p 用户名:#{user.username}
      p 学号:#{user.studentId}
      p 电话:#{user.phone}
      p 邮箱:#{user.mail}
      a(href='/signout')
        input#signout(type='button', value='退出')
  if errorMsg
    #remind
      p #{errorMsg}
